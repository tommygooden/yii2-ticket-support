{
  "name": "tommygooden/yii2-ticket-support",
  "description": "A fork of Yii2 Support Ticket Module, easy, flexible, fast by Akiraz2",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "support",
    "ticket",
    "module",
    "tommy",
    "gooden",
    "jamaica"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tommy Gooden",
      "email": "tommygooden@hotmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "yidas/yii2-bower-asset": "^2.0.5",
    "yiisoft/yii2": "^2.0",
    "hashids/hashids": "^5.0.2",
    "yiidoc/yii2-redactor": "*",
    "php-imap/php-imap": "^5.0",
    "yiisoft/yii2-queue": "^2.0",
    "rmrevin/yii2-fontawesome": "^3.7",
    "yiisoft/yii2-symfonymailer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "akiraz2\\support\\": ""
    }
  },
  "extra": {
    "bootstrap": "akiraz2\\support\\Bootstrap"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
